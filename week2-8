#include<iostream>
using namespace std;  

void swap(int &x, int &y){
        int temp;
        temp=x;
        x=y;
        y=temp;
}
int main()
{
        int x,y;
        cout << "Enter the numbers: ";
        cin >> x >> y;
        cout << "Before swapping"<<"\n X = " << x << "\n Y = " << y << endl;
        swap(x,y);
        cout << "After swapping"<<"\n X = "<< x << "\n Y = " << y << endl;
        return 0;
}
