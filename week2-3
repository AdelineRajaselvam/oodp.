#include <iostream>
using namespace std;
int
main ()
{
  char choice;
  int num1, num2;
  cout << "+ , - , * , / , %" << endl;
  cout << "Choose operator:";
  cin >> choice;
  cout << "Enter two operands: ";
  cin >> num1 >> num2;
  switch (choice)
    {
    case '+':
      cout << num1 + num2;
      break;
    case '-':
      cout << num1 - num2;
      break;
    case '*':
      cout << num1 * num2;
      break;
    case '/':
      cout << num1 / num2;
      break;
    case '%':
      cout << num1 % num2;
      break;
    default:
      cout << "Error operator is not correct";
      break;
    }
  return 0;
}
